<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!--mapper映射-->
<mapper namespace="com.gameloft9.demo.dataaccess.dao.system.TpQueAccountMapper">

    <!--查询输入的账号-->
    <select id="findAccount" parameterType="java.lang.String" resultType="com.gameloft9.demo.dataaccess.model.system.TpQueVerification">
        SELECT tp_democratic_verification.*
        FROM tp_democratic_verification
        WHERE tp_democratic_verification.content=#{content,jdbcType=VARCHAR}
    </select>
</mapper>
